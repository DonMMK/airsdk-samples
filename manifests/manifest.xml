<?xml version="1.0" encoding="UTF-8"?>
<manifest>
 <remote name="gerrit" fetch="ssh://gerrit-doc.parrot.biz" review="gerrit-doc.parrot.biz"/>

    <default remote="gerrit" revision="master" upstream="master" sync-j="4"/>

    <!-- dragon build  -->
    <project name="build/Alchemy" path="build/alchemy"/>
    <project name="drones/dragon_build" path="build/dragon_build">
        <linkfile src="root-build.sh" dest="build.sh"/>
    </project>

    <!-- Air SDK Doc -->
    <project name="airsdk-doc" path="packages/airsdk-doc">
        <linkfile src="samples/hello/product" dest="products/hello"/>
        <linkfile src="samples/README.md" dest="README"/>
    </project>
</manifest>
